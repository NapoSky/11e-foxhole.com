{
  "name": "11e-foxhole",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "yarn compile && yarn next build",
    "clean": "rm -rf build-tsc .next",
    "compile": "yarn run -T tsc --build --verbose",
    "dev": "yarn compile && yarn next dev",
    "lint": "yarn run -T prettier --write './src/**/*.{js,jsx,json,ts,tsx}' && eslint './src/**/*.{js,jsx,ts,tsx}' --fix --max-warnings=0 --format=json > /dev/null",
    "start": "yarn next start",
    "sitemap": "yarn next-sitemap"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "24.0.14",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.4.21",
    "classnames": "^2.5.1",
    "country-flag-icons": "^1.5.19",
    "cssnano": "^7.0.7",
    "fs-extra": "^11.3.0",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "next": "^15.3.5",
    "next-i18next": "^15.4.2",
    "next-images": "^1.8.5",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.2.4",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.6.0",
    "sass": "^1.89.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3"
  },
  "resolutions": {
    "string-width": "^7.1.2",
    "string-width-cjs": "^5.1.1",
    "strip-ansi": "^7.0.1",
    "strip-ansi-cjs": "^8.0.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.3.5",
    "@types/eslint": "^9.6.1",
    "@types/eslint-scope": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.31.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "webpack": "^5.100.1"
