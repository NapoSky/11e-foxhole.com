{
  "name": "11e-foxhole",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "yarn compile && yarn next build",
    "clean": "rm -rf build-tsc .next",
    "compile": "yarn run -T tsc --build --verbose",
    "dev": "yarn compile && yarn next dev",
    "lint": "yarn run -T prettier --write './src/**/*.{js,jsx,json,ts,tsx}' && eslint './src/**/*.{js,jsx,ts,tsx}' --fix --max-warnings=0",
    "start": "yarn next start",
    "sitemap": "yarn next-sitemap"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.2",
    "@heroicons/react": "^2.1.5",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/node": "22.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "classnames": "^2.5.1",
    "cssnano": "^7.0.4",
    "fs-extra": "^11.2.0",
    "i18next": "^23.12.2",
    "i18next-browser-languagedetector": "^8.0.0",
    "next": "^14.2.5",
    "next-i18next": "^15.3.1",
    "next-images": "^1.8.5",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.4.40",
    "postcss-preset-env": "^9.6.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-country-flag": "^3.1.0",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.0",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4"
  },
  "resolutions": {
    "string-width": "^5.1.2",
    "string-width-cjs": "^5.1.1",
    "strip-ansi": "^6.0.0",
    "strip-ansi-cjs": "^8.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^9.8.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2"
  }
}
