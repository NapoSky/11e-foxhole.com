{
  "name": "11e-foxhole",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "yarn compile && yarn next build",
    "clean": "rm -rf build-tsc .next",
    "compile": "yarn run -T tsc --build --verbose",
    "dev": "yarn compile && yarn next dev",
    "lint": "yarn run -T prettier --write './src/**/*.{js,jsx,json,ts,tsx}' && eslint './src/**/*.{js,jsx,ts,tsx}' --fix --max-warnings=0 --format=json > /dev/null",
    "start": "yarn next start",
    "sitemap": "yarn next-sitemap"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.10",
    "@heroicons/react": "^2.1.5",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "22.7.7",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "classnames": "^2.5.1",
    "country-flag-icons": "^1.5.13",
    "cssnano": "^7.0.6",
    "fs-extra": "^11.2.0",
    "i18next": "^23.16.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "next": "^14.2.15",
    "next-i18next": "^15.3.1",
    "next-images": "^1.8.5",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.4.47",
    "postcss-preset-env": "^10.0.7",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.3",
    "sass": "^1.80.3",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3"
  },
  "resolutions": {
    "string-width": "^7.1.2",
    "string-width-cjs": "^5.1.1",
    "strip-ansi": "^7.0.1",
    "strip-ansi-cjs": "^8.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/eslint-scope": "^3.7.7",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^9.13.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "webpack": "^5.95.0"
  }
